{
    "version": 2,
    "builds": [
        {
            "src": "anthill_iq/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "python manage.py migrate --noinput && python manage.py collectstatic --noinput"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "anthill_iq/wsgi.py"
        }
    ]
} 