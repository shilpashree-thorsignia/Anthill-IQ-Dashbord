{
    "version": 2,
    "builds": [
        {
            "src": "anthill_iq/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "anthill_iq/wsgi.py"
        }
    ],
    "postinstall": "python manage.py collectstatic --noinput"
} 